  },
  "homepage": "https://github.com/diuguide/valuePal#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1"
    
  }
}
